        <channel:position type="OUTPUT" />
    </device:phidgetAdvancedServo>
    <device:phidgetAdvancedServo serial="303292" index="1" name="LeftEyeServo1">
        <channel:position type="OUTPUT" />
    </device:phidgetAdvancedServo>
    <device:phidgetAdvancedServo serial="303292" index="2" name="NeckServo">
        <channel:position type="OUTPUT" />
    </device:phidgetAdvancedServo>
