<deviceGroup name="tactile" skip="false">
    <device:simpleTactileSensor path="/dev/ttyUSB0" index="1" name="LeftHandTactileSensor">
        <channel:value type="INPUT" />
    </device:simpleTactileSensor>
</deviceGroup>
</config>
