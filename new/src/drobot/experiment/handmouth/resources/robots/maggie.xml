        <deviceGroup name="head">

            <device:phidgetAdvancedServo serial="303292" index="5" name="neckServo" skip="true">
                <channel:position type="OUTPUT" />
            </device:phidgetAdvancedServo>
            
            <deviceGroup name="vision" skip="true">
                <device:phidgetSimpleServo serial="09818" index="0" name="leftEyePanServo">
